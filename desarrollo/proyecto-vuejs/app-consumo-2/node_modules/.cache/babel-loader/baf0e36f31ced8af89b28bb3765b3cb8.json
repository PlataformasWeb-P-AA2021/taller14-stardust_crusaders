            }\r\n                console.log(this.direccion)\r\n                axios.post('http://127.0.0.1:8000/api/edificios/',\r\n                        this.edificio\r\n                    )\r\n                    .then(response => {\r\n                        this.$router.push('/');\r\n                    })\r\n            });\r\n        }\r\n    },\r\n}\r\n</script>\r\n"],"sourceRoot":"src/components"}]}