 'axios';\r\n\r\nexport default {\r\n    data() {\r\n        return {\r\n            edificios: []\r\n        }\r\n    },\r\n    created() {\r\n        this.all();\r\n    },\r\n    methods: {\r\n        deleteEdificio: function(e) {\r\n            if (confirm('Eliminar ' + e.nombre)) {\r\n                axios.delete(e.url)\r\n                    .then( response => {\r\n                        this.all();\r\n                    });\r\n            }\r\n        },\r\n        all: function () {\r\n            axios.get('http://127.0.0.1:8000/api/edificios/')\r\n                .then( response => {\r\n                    this.edificios = response.data['results']\r\n                });\r\n        }\r\n    },\r\n}\r\n</script>\r\n"],"sourceRoot":"src/components"}]}